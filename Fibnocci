/Program to find n'th fibnocci number using recursion
#include<bits/stdc++.h>
using namespace std;
 int Fib(int n)
{
    if (n <= 1)
    {
         return n;
    }
    return Fib(n-1)+Fib(n-2);
}
 
int main ()
{
    int n;
    cout<<"Enter a number:";
    cin>>n;
    cout <<"The Fibnocci number is:"<<Fib(n);
    return 0;
}
